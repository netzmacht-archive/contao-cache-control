{
    "name":"netzmacht/contao-cache-control",
    "description": "Clear the Contao page cache for single files.",
    "keywords":["contao", "page", "cache"],
    "type":"contao-bundle",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"David Molineus",
            "email":"mail@netzmacht.de",
            "homepage":"https://netzmacht.de",
            "role":"Developer"
        }
    ],
    "support":{
        "email":"mail@netzmacht.de",
        "issues":"https://github.com/netzmacht/contao-cache-control/issues",
        "source":"https://github.com/netzmacht/contao-cache-control"
    },
    "require":{
        "php":">=5.6",
        "contao/core-bundle": "~3.5 || ~4.4",
        "contao-community-alliance/composer-plugin": "~2.0 || ~3.0",
        "friendsofsymfony/http-cache-bundle": "~2.0"
    },
    "require-dev": {
        "netzmacht/contao-build-tools": "~1.1",
        "contao/manager-plugin": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Netzmacht\\Contao\\CacheControl\\": "src/"
        }
    },
    "extra":{
        "contao-manager-plugin": "Netzmacht\\Contao\\CacheControl\\ContaoManager\\Plugin",
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "2.0.x-dev"
        }
    }
}
